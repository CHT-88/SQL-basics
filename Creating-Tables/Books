CREATE TABLE Books
(ISBN VARCHAR(10) Primary Key,
Book_Title VARCHAR(500) NOT NULL,
Book_Author VARCHAR(50) NOT NULL,
Year_of_Publication INTEGER NOT NULL,
Publisher VARCHAR(50) NOT NULL,


)

INSERT INTO Books (ISBN,	Book_Title)
VALUES (,'')
